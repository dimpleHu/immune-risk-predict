<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.immuserisk.dao.SymptomsDao">
    <resultMap id="SymptomsResult" type="com.immuserisk.entity.Symptoms">
        <id column="symptomID" property="symptomID"/>
        <result column="symptomName" property="symptomName"/>
        <result column="affectedID" property="affectedID"/>
    </resultMap>
    <!-- 查询所有症状 -->
    <select id="findAll" resultType="com.immuserisk.entity.Symptoms">
        SELECT * FROM symptoms
    </select>

    <!-- 根据ID查询症状 -->
    <select id="findById" resultType="com.immuserisk.entity.Symptoms">
        SELECT * FROM symptoms WHERE symptomID = #{symptomsID}
    </select>

    <!-- 根据名称模糊查询症状 -->
    <select id="findByName" resultType="com.immuserisk.entity.Symptoms">
        SELECT * FROM symptoms WHERE symptomName LIKE CONCAT('%', #{symptomName}, '%')
    </select>
</mapper>